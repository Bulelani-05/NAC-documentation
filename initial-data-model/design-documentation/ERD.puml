' Entity Relationship Diagram generated from DBML

' Table: follows
entity follows {
  following_user_id : integer
  followed_user_id : integer
  created_at : timestamp
}

' Table: users
entity users {
  id : integer <<PK>>
  username : varchar
  role : varchar
  created_at : timestamp
}

' Table: posts
entity posts {
  id : integer <<PK>>
  title : varchar
  body : text
  user_id : integer
  status : varchar
  created_at : timestamp
}

