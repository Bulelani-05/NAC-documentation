  MoveDate : date
  Preferred : tinyint(1)
  Dispatch : tinyint(1)
  CreatedOn : timestamp
  CreatedBy : varchar(100)
  ChangedOn : timestamp
  ChangedBy : varchar(100)
  FbId : int(11)
}

entity AllAreas {
  Id : int(11)
